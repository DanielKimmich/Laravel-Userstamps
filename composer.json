{
    "name": "dalisoft/userstamps",
    "description": "Laravel Userstamps provides an Eloquent trait which automatically maintains `created_by` and `updated_by` columns on your model, populated by the currently authenticated user in your application.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "eloquent",
        "userstamps",
        "created_by",
        "updated_by",
        "deleted_by"
    ],
    "authors": [
        {
            "name": "Daniel Kimmich",
            "email": "danielkimmich72@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "DaLiSoft\\Userstamps\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DaLiSoft\\Userstamps\\UserStampServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
